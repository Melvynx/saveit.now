{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "env": [
        "GITHUB_CLIENT_ID",
        "RESEND_API_KEY",
        "OPENAI_API_KEY",
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY",
        "AWS_S3_BUCKET_NAME",
        "AWS_REGION",
        "GITHUB_CLIENT_SECRET",
        "BETTER_AUTH_SECRET",
        "DATABASE_URL",
        "DATABASE_URL_UNPOOLED",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_SECRET",
        "GOOGLE_GENERATIVE_AI_API_KEY",
        "AWS_ENDPOINT"
      ],
      "dependsOn": ["^build", "^db:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:deploy": {
      "env": ["DATABASE_URL", "DATABASE_URL_UNPOOLED"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "cache": false
    },
    "inngest:dev": {
      "cache": false
    }
  }
}
